{
  "expo": {
    "owner": "aufabi",
    "name": "Vuluz",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vuluz.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/V.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "44d3f0ab-08c1-45f5-a885-86bf583a4a23"
      }
    },
    "android": {
      "package": "com.evelio.excellenta.boltexponativewindp",
      "adaptiveIcon":{
        "foregroundImage": "./assets/images/vuluz.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
